{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[python]": {
    "editor.defaultFormatter": null
  },
  "editor.formatOnSave": true,
  "python.formatting.provider": "black",
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "cSpell.ignoreWords": [
    "algs",
    "asyncssh",
    "bumpver",
    "bumpversion",
    "cpurevision",
    "diffie",
    "hwaddr",
    "hwaddrs",
    "isort",
    "mpower",
    "pasbec",
    "powerfactor",
    "pylint",
    "pyproject",
    "pytest",
    "setuptools",
    "vpower"
  ],
  "cSpell.words": [
    "Dropbear",
    "Ubiquiti",
    "WLAN"
  ]
}
